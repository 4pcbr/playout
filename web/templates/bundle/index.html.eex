<h1>bundles</h1>

<a href="<%= bundle_path( @conn, :new ) %>" class="btn btn-primary">New</a>

<table class="table">
  <%= for bundle <- @bundles do %>
    <tr>
      <td><%= bundle.id %></td>
      <td><%= bundle.name %></td>
      <td><%= link "Edit", to: bundle_path( @conn, :edit, bundle.id ) %></td>
    </tr>
  <% end %>
</table>
